#include<stdio.h>
int IsPrime(int);
void main()
{
int num,res;
printf("enter a number:r);
scanf("%d",&num);
res=IsPrime(num);
if(res==0)
{
printf("its a prime number");
}
else
{
printf("its not a prime number");
}
}
int IsPrime(int n)
{
int i;
for(i=2;i<=n/2;i++)
{
if(n%i!=0)
continue;
return 1;
}
return 0;
}

